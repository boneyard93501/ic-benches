# Namespace-scoped credentials. One block per provider namespace from config.toml
# Example provider: [[providers]] id="ic-eu", namespace="IC_EU"
IC_EU_ACCESS_KEY=
IC_EU_SECRET_KEY=
IC_EU_SESSION_TOKEN=

# Example: id="ic-us", namespace="IC_US"
IC_US_ACCESS_KEY=
IC_US_SECRET_KEY=
IC_US_SESSION_TOKEN=

# Optional AWS-style fallback (last resort)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=
